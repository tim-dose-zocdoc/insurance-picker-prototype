<div>
    <h3>popular carriers like <span class="query">{{query}}</span></h3>
    <ul class="list">
        {{#popular}}
            <li class="list__item has-next" data-carrier-id="{{id}}">
                <span class="item">{{carrier}}</span>
                <div class="icons">
	                <span class="icon icon-next"><i class="fa fa-chevron-right"></i></span>
	                <span class="icon icon-selected"><i class="fa fa-check"></i></span>
                </div>
            </li>
        {{/popular}}
    </ul>

    <h3>all carriers like <span class="query">{{query}}</span></h3>
    <ul class="list">
        {{#all}}
            <li class="list__item has-next" data-carrier-id="{{id}}">
                <span class="item">{{carrier}}</span>
                <div class="icons">
                    <span class="icon icon-next"><i class="fa fa-chevron-right"></i></span>
                    <span class="icon icon-selected"><i class="fa fa-check"></i></span>
                </div>
            </li>
        {{/all}}
    </ul>
</div>
